<% @listings.each do |t| %>
	<%= t.name %>
	<%= t.location %>
	RM <%= t.price %>
<% end %>

<% if current_user.admin? || current_user.host? %>
	<%= link_to "Create new listing", new_listing_path %>
<% end %>
<% if current_user.admin? || current_user.moderator? %>
	<%= link_to "Unverified Listings", admin_listings_path %>
<% end %>